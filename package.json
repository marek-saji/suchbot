{
  "name": "suchbot",
  "version": "1.0.0",
  "description": "A bot.",
  "author": "Marek ‘saji’ Augustynowicz <marek.aug@gmail.com> (https://twitter.com/saji_)",
  "license": "ISC",
  "keywords": [
    "slack",
    "bot",
    "lunch"
  ],
  "homepage": "https://github.com/marek-saji/suchbot",
  "bugs": "https://github.com/marek-saji/suchbot/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/marek-saji/suchbot.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node --harmony index.js",
    "test": "node_modules/.bin/eslint index.js"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "google-spreadsheets": "^0.4.2",
    "googleapis": "1.*",
    "iconv": "^2.1.11",
    "slack-client": "^1.4.1"
  },
  "devDependencies": {
    "eslint": "^1.6.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "indent": [
        2,
        4
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "yoda": [
        2,
        "always",
        {
          "onlyEquality": true
        }
      ]
    }
  }
}
